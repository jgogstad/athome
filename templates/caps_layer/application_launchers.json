{
  "description": "Hyper Application",
  "manipulators": [
    {
      "type": "basic",
      "from": {
        "key_code": "y",
        "modifiers": {
          "mandatory": [
            "left_command"
          ]
        }
      },
      "to": [
        {
          "shell_command": "open -a 'Typora'"
        }
      ],
      "conditions": [{
        "type": "variable_if",
        "name": "caps_lock pressed",
        "value": 1
      }]
    },
